{
  "name": "n8n-nodes-draiven",
  "version": "0.1.2",
  "description": "n8n node for Draiven AI integration",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "draiven",
    "ai",
    "analytics"
  ],
  "license": "MIT",
  "homepage": "https://draiven.io",
  "author": {
    "name": "Draiven",
    "email": "support@draiven.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/draiven-io/n8n-nodes-draiven.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint credentials/**/*.ts nodes/**/*.ts package.json",
    "lintfix": "eslint credentials/**/*.ts nodes/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DraivenApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Draiven/Draiven.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.40.0",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
